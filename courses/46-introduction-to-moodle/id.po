# "a3e3439748c4d67bc7ccce9f808bcc68"}
msgid "Introduction to Moodle: Check your understanding"
msgstr "Pengantar Moodle: Periksa pemahaman Anda"

# {"targetlanguage": "id", "contextid": "6965", "md5key":
# "19d4d0f261eff89ed4abb6903fd6976b"}
msgid ""
"<div class=\"no-overflow\"><div class=\"no-overflow\"><p dir=\"ltr\" "
