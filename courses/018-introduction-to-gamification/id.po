msgid "<p dir=\"ltr\" style=\"text-align: left;\">General education</p>"
msgstr ""

# {"targetlanguage": "id"}
#: 18397dd9688a6aa2053e4779e98d65da:2981
msgid ""
"<p dir=\"ltr\" style=\"text-align: left;\">Hopefully you selected this "
"one!</p>"
msgstr ""

