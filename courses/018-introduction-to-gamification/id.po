#: c9a5215a48963fe7626771280289414c:2981
msgid "<p dir=\"ltr\" style=\"text-align: left;\">134%</p>"
msgstr ""

# {"targetlanguage": "id"}
#: 56dd04159ea21308102d89e5ea77374a:2981
msgid "In student participation types, what are disruptors?"
